import java.util.Scanner;
class Main{
    public static void main(String[] args) {
    int A=10;
    int B= 50;
    int C=30;
    if (A>B && A>C){
        System.out.println("A is greatest");
    }
    else if(B>=C){
        System.out.println("B is greatest");
    }
    else{
        System.out.println(" C is greatest");
    }

    }
}
